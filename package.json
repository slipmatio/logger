{
  "name": "@slipmatio/logger",
  "version": "0.0.8",
  "description": "Better console logging with TypScript support.",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Ville Säävuori",
    "email": "ville@unessa.net"
  },
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:types": "tsc --build tsconfig.json",
    "cleanup": "bash scripts/cleanup.sh",
    "lint": "prettier -c --parser typescript \"{src,tests,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "release": "bash scripts/release.sh",
    "size": "size-limit",
    "test:unit": "jest --coverage",
    "test": "yarn run build:types && yarn run test:unit"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "**/*.d.ts.map",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "logger",
    "logging",
    "console",
    "vue",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "jest": "^26.6.3",
    "pascalcase": "^1.0.0",
    "prettier": "^2.1.2",
    "size-limit": "^4.7.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "vue": "^3.0.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-push": "yarn lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/slipmatio/logger.git"
  },
  "bugs": {
    "url": "https://gitlab.com/slipmatio/logger/-/issues"
  },
  "homepage": "https://gitlab.com/slipmatio/logger"
}
