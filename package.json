    "@vue/test-utils": "2.4.0",
    "happy-dom": "9.20.3",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vite-plugin-dts": "3.0.3",
    "vitest": "0.32.4",
    "vue": "3.3.4",
    "vue-tsc": "1.8.3"
  },
  "engines": {
