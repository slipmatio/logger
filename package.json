{
  "name": "@slipmatio/logger",
  "version": "0.0.5",
  "description": "Better console logging with TypScript support.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm-bundler.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Ville Säävuori",
    "email": "ville@unessa.net"
  },
  "scripts": {
    "build": "bash scripts/build.sh",
    "release": "bash scripts/release.sh",
    "size": "size-limit",
    "lint": "prettier -c --parser typescript \"{src,tests,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "jest --coverage",
    "test": "yarn run test:types && yarn run test:unit && yarn run build"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "logger",
    "logging",
    "console",
    "vue",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "esbuild": "^0.8.4",
    "jest": "^26.6.3",
    "pascalcase": "^1.0.0",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-esbuild": "^2.5.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "size-limit": "^4.7.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "vue": "^3.0.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-push": "yarn lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/slipmatio/logger.git"
  },
  "bugs": {
    "url": "https://gitlab.com/slipmatio/logger/-/issues"
  },
  "homepage": "https://gitlab.com/slipmatio/logger"
}
